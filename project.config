[project]
	description = This is an active mirror of Jersey 2.x workspace from http://jersey.java.net. Any changes made here are automatically propagated to java.net and vice versa. Forks and pull requests are welcome!
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Khaled Ayoubi (kayoubi)
[access "refs/for/refs/*"]
	read = group user/Khaled Ayoubi (kayoubi)
	push = group user/Khaled Ayoubi (kayoubi)
	label-Code-Review = -2..+2 group user/Khaled Ayoubi (kayoubi)
	label-Verified = -1..+1 group user/Khaled Ayoubi (kayoubi)
	rebase = group user/Khaled Ayoubi (kayoubi)
	removeReviewer = group user/Khaled Ayoubi (kayoubi)
	submit = group user/Khaled Ayoubi (kayoubi)
[access "refs/heads/*"]
	read = group user/Khaled Ayoubi (kayoubi)
	create = group user/Khaled Ayoubi (kayoubi)
	push = +force group user/Khaled Ayoubi (kayoubi)
	pushMerge = group user/Khaled Ayoubi (kayoubi)
[access "refs/tags/*"]
	push = +force group user/Khaled Ayoubi (kayoubi)
	pushTag = group user/Khaled Ayoubi (kayoubi)
	pushSignedTag = group user/Khaled Ayoubi (kayoubi)
